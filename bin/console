#!/usr/bin/env ruby
# frozen_string_literal: true

require 'bundler/setup'
require 'xmltools'

puts 'starting console...'

# Load default config
Xmltools::ConfigLoader.new.call

# (If you use this, don't forget to add pry to your Gemfile!)
require 'pry'
Pry.start

